<div class="container">
  <h1>Artists</h1>
  <div class="search-pic-container">
    <% for (const artist of artists.sort(() => .5 - Math.random())) { %>
      <% if (artist.place) continue; %>
      <a href="/artists/<%= artist._id %>" class="search-pic">
        <img src="<%= artist.profilePic %>">
        <div class="gradient"></div>
        <span><%= artist.name %></span>
      </a>
    <% } %>
  </div>
</div>
